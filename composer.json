{
    "name": "rockers/plaid-stripe",
    "description": "its sdk for plaid stripe payment process.",
    "type": "library",
    "require": {
        "php": ">=5.6",
        "laravel/framework": "6.0",
        "cartalyst/stripe-laravel": "11.0.*"
    },
     "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "3.9.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Rakesh Patel",
            "email": "rakesh@rockersinfo.com"
        }
    ],
    "minimum-stability": "dev",
     "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Rockers\\PlaidStripe\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rockers\\PlaidStripe\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rockers\\PlaidStripe\\PlaidStripeServiceProvider"
            ]
        }
    }
}

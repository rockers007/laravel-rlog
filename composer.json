{
    "name": "rakesh/laravel-rlog",
    "description": "its log for all laravel projects.so its make debugging very easy.",
    "type": "library",
    "require": {
        "php": "7.1",
        "illuminate/support": "5.6",
        "monolog/monolog": "1.23.*"
    },
     "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "3.9.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Rakesh Patel",
            "email": "rakesh@rockersinfo.com"
        }
    ],
    "minimum-stability": "dev",
     "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Rockersweb\\LaravelRlog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rockersweb\\LaravelRlog\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rockersweb\\LaravelRlog\\LaravelRlogServiceProvider"
            ]
        }
    }
}
